<?xml version="1.0"?>

<dspace-dc-types>
    <dspace-header>
        <title>Cristin Incoming Metadata Registry</title>
    </dspace-header>
    
    <dc-schema>
        <name>dc</name>
        <namespace>http://dublincore.org/documents/dcmi-terms/</namespace>
    </dc-schema>

    <dc-type>
        <schema>dc</schema>
        <element>creator</element>
        <qualifier>author</qualifier>
        <scope_note/>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>identifier</element>
        <qualifier>cristinID</qualifier>
        <scope_note>CRIStins løpenummer for metadata - posten.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>identifier</element>
        <qualifier>doi</qualifier>
        <scope_note>Document Object Identifier.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>identifier</element>
        <qualifier>bibliographicCitation</qualifier>
        <scope_note>Referanse til f.eks. tidsskrift som artikkel er i eller bok som kapittel inngår i.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>relation</element>
        <qualifier>projectID</qualifier>
        <scope_note>[kilde]: prosjekt - ID.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>source</element>
        <qualifier>isbn</qualifier>
        <scope_note>ISBN for bok som kapittel er i.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>source</element>
        <qualifier>issn</qualifier>
        <scope_note>ISSN for tidsskrift.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>subject</element>
        <qualifier>keyword</qualifier>
        <scope_note>Fritt emneord.</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>subject</element>
        <qualifier>nsi</qualifier>
        <scope_note>Norwegian science index</scope_note>
    </dc-type>

    <dc-type>
        <schema>dc</schema>
        <element>type</element>
        <qualifier>document</qualifier>
        <scope_note>CRIStin - postens kategori.</scope_note>
    </dc-type>
</dspace-dc-types>
